{
  "name": "bitlogin",
  "version": "0.3.0",
  "description": "Bitlogin, access apps with Bitcoin",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "target": "src/index.js",
  "scripts": {
    "test": "rm -rf dist/*.* && NODE_ENV=development parcel test/test.html img/** --port 3000",
    "test2": "rm -rf dist/*.* && NODE_ENV=development parcel test/test2.html img/** --port 3000",
    "build": "parcel build src/index.js",
    "build2": "browserify src/index.js -s bitlogin > dist/index.js",
    "update": "scp dist/index.js bsv.direct:/srv/www/bsv.direct/lib/bitlogin/index.js"
  },
  "author": "Tomas Diaz",
  "license": "ISC",
  "dependencies": {
    "bsv": "^2.0.10",
    "dotenv": "^16.0.1",
    "pure-css-loader": "^3.3.3"
  },
  "devDependencies": {
    "babelify": "^10.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "esmify": "^2.1.1",
    "events": "^3.3.0",
    "minify": "^9.1.0",
    "minifyify": "^7.3.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "tinyify": "^3.1.0",
    "uglifyify": "^5.0.2",
    "util": "^0.12.4"
  }
}
